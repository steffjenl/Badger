{
    "name": "cachethq/badger",
    "description": "A Badge Generator For Laravel 5 and 6 and 7.",
    "keywords": ["badger", "badge", "shield", "svg", "Badger", "Cachet", "CachetHQ"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cachet",
            "email": "support@cachethq.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.1",
        "graham-campbell/testbench": "^5.1",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CachetHQ\\Badger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CachetHQ\\Tests\\Badger\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "CachetHQ\\Badger\\BadgerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
